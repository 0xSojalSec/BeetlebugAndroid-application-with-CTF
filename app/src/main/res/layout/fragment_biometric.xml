<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:gravity="center_horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:padding="15dp">

        <ImageView
            android:id="@+id/back"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:onClick="goBack"
            android:src="@drawable/left3" />

        <TextView
            android:id="@+id/insecure_data_storage_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/kanit_regular"
            android:text="Biometric By-Pass"
            android:textColor="#000"
            android:textSize="27sp" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="400dp"
        android:layout_height="148dp"
        android:layout_marginTop="87dp"
        android:padding="10dp"
        android:layout_marginBottom="20dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/ctfTitle"
            android:layout_width="255dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/kanit_regular"
            android:text="Weak Deeplink By-Pass"
            android:textColor="@android:color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textDescription"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ctfTitle"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/kanit_regular"
            android:text="Fingerprint authentication can be circumvented via weak deeplinks"
            android:textSize="14sp" />

        <Button
            android:id="@+id/button"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:background="@drawable/green_button"
            android:fontFamily="@font/kanit_regular"
            android:text="Start"
            android:textAllCaps="false"
            android:textColor="@android:color/black"
            android:textSize="12sp" />


    </RelativeLayout>

